{
  "name": "drunk-buds",
  "version": "1.0.0",
  "description": "An app to make sure you don't lose your drunk friends!",
  "main": "index.js",
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "start": "npm run server",
    "start:dev": "npm run server:dev & npm run build:dev",
    "server": "node ./server/index.js",
    "server:dev": "./node_modules/.bin/nodemon ./server/index.js --ignore public/ --ignore src/",
    "build": "./node_modules/.bin/webpack --config webpack.production.config.js",
    "build:dev": "./node_modules/.bin/webpack --config webpack.config.js --watch",
    "seed": "node ./seed.js"
  },
  "keywords": [
    "react",
    "redux",
    "bulma",
    "node",
    "express",
    "postgresql",
    "sequelize",
    "bcrypt"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^6.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "google-map-react": "^1.1.4",
    "helmet": "^3.20.0",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-map-gl": "^5.0.10",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.16.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "nodemon": "^1.19.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  }
}
